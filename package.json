{
  "name": "fullstack-express-react-mongodb-app",
  "version": "1.0.0",
  "description": "Minimal Fullstack MERN app build with Express, React and MongoDB.",
  "author": "Alex Devero",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=7.0.0",
    "yarn": ">=1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "app.js",
  "scripts": {
    "start-client": "react-scripts start",
    "build-client": "react-scripts build",
    "test-client": "react-scripts test",
    "eject-client": "react-scripts eject",
    "start": "node app.js",
    "heroku-postbuild": "cd client && yarn install && yarn build",
    "client": "cd client && yarn start",
    "server": "nodemon server/app.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "nodemon": "^2.0.6",
    "react-scripts": "4.0.1"
  }
